#!/usr/bin/python
# -*- coding:utf-8 -*-
# network
Inputs("input", "label")
Outputs("cost")

Layer(
    name = "input",
    type = "data",
    size = 2304,
)

Layer(
    name = "conv1_1",
    type = "exconv",
    device = 0,
    active_type = "relu",
    bias = Bias(learning_rate=0.001,
        momentum=0.9,
        initial_mean=0,
        initial_std=0.0),
    inputs = Input("input",
        learning_rate=0.001,
        momentum=0.9,
        decay_rate=0.0004,
        initial_mean=0.0,
        initial_std=0.0005,
        conv = Conv(filter_size = 3,
             channels = 1,
             padding = 1,
             stride = 1,
             groups = 1)),
    num_filters = 16,
    partial_sum = 1,
    shared_biases = True
)
Layer(
    name = "conv1_2",
