    #active_type = "softmax",
    bias = Bias(learning_rate=0.001,
        momentum=0.9,
        initial_mean=0,
        initial_std=0),
    inputs = [Input("gated1",
        learning_rate=0.001,
        momentum=0.9,
        decay_rate=0.0004,
        initial_mean=0.0,
        initial_std=0.02),
              Input("gated2",
