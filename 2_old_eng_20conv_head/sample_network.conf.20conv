    size = 0,
)

Layer(
    name = "cost",
    type = "warp_ctc",
    device = 0,
    size = 98,
    norm_by_times = True,
    inputs = ["output", "label"],
)

Evaluator(
    name = "error",
    type = "ctc_edit_distance",
    inputs = ["output", "label"],
)
