Layer(
    name = "expand4",
    type = "blockexpand",
    device = 0,
    inputs = Input("conv4_5",
        block_expand = BlockExpand(
            channels = 128,
            padding_x = 0,
            padding_y = 0,
            stride_x = 1,
            stride_y = 1,
            block_x = 1,
            block_y = 6))
)

Layer(
    name = "fc5",
    type = "fc",
    device = 0,
    size = 600,
    inputs = Input("expand4",
        learning_rate=0.001,
        momentum=0.9,
        decay_rate=0.0004,
        initial_mean=0.0,
        initial_std=0.02),
)

Layer(
    name = "gated1",
    type = "gated_recurrent",
    device = 0,
    active_type = "relu",
    active_gate_type = "sigmoid",
    reversed = False,
    bias = Bias(learning_rate=0.002, # 0.002
        momentum=0.9,
        decay_rate=0.0004,
        initial_mean=0.0,
        initial_std=0.02),
    inputs = Input("fc5",
        learning_rate=0.001,
        momentum=0.9,
        decay_rate=0.0004,
        initial_mean=0.0,
        initial_std=0.02),
)

Layer(
    name = "fc6",
    type = "fc",
    device = 0,
    size = 600,
    inputs = Input("expand4",
        learning_rate=0.001,
        momentum=0.9,
        decay_rate=0.0004,
        initial_mean=0.0,
        initial_std=0.02),
)

Layer(
    name = "gated2",
    type = "gated_recurrent",
    device = 0,
    active_type = "relu",
    active_gate_type = "sigmoid",
    reversed = True,
    bias = Bias(learning_rate=0.002, 
        momentum=0.9,
        decay_rate=0.0004,
        initial_mean=0.0,
        initial_std=0.02),
    inputs = Input("fc6",
        learning_rate=0.001,
        momentum=0.9,
        decay_rate=0.0004,
